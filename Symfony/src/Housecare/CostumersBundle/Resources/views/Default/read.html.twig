{# empty Twig template #}
{% set current_page = 'read' %}
{% extends "HousecareWorkersBundle:workers:layout.html.twig" %}




{% block container %}


<div class="page-header">
   <h1>Create a new costumers</h1>
</div>
<p>
    <a href="{{ path('housecare_costumers_list') }}" class="btn">
      <i class="icon-chevron-left"></i>
      Back to the list
    </a>
    <a href="{{ path('housecare_costumers_list') }}" class="btn">
      <i class="icon-edit"></i>
      Edit the worker
    </a>
    <a href="" class="btn disabled">
      <i class="icon-trash "></i>
      Delete the worker
    </a>
  </p>

<div class="row-fluid">
   <div class="span4">
      <dl>
         <dt>Last Name</dt>
         <dd>{{ object.lastName }}</dd>
         <dt>First Name</dt>
         <dd>{{ object.firstName }}</dd>
         <dt>Phone</dt>
         <dd>{{ object.phone }}</dd>
         
      </dl>
   </div>
   <div class="span4">

      <dl>
         <dt>Street</dt>
         <dd>{{ object.street}}</dd>
         <dt>Number</dt>
         <dd>{{ object.number }}</dd>
         <dt>Zip Code</dt>
         <dd>{{ object.zipCode }}</dd>
         <dt>City</dt>
         <dd>{{ object.city }}</dd>
         <dt>Country</dt>
         <dd>{{ object.country }}</dd>
      </dl>
   </div>
      <div class="span4">
         <h3>Note</h3>
         {{ object.note }}
      </div>
</div>


{% endblock %}