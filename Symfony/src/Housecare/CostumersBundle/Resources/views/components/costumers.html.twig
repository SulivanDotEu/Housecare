   <h1>Costumer #{{ costumer.id }}</h1>
</div>
<p>
    <a href="{{ path('housecare_costumers_list') }}" class="btn">
      <i class="icon-chevron-left"></i>
      Back to the list
    </a>
    <a href="{{ path('housecare_costumers_list') }}" class="btn">
      <i class="icon-edit"></i>
      Edit the worker
    </a>
    <a href="" class="btn disabled">
