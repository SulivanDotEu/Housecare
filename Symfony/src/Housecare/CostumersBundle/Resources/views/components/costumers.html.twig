         <dd>{{ costumer.phone }}</dd>
         
      </dl>
   </div>
   <div class="span4">

      <dl>
         <dt>Street</dt>
         <dd>{{ costumer.street}}</dd>
         <dt>Number</dt>
         <dd>{{ costumer.number }}</dd>
         <dt>Zip Code</dt>
         <dd>{{ costumer.zipCode }}</dd>
         <dt>City</dt>
         <dd>{{ costumer.city }}</dd>
         <dt>Country</dt>
         <dd>{{ costumer.country }}</dd>
      </dl>
   </div>
      <div class="span4">
         <h3>Note</h3>
         {{ costumer.note }}
      </div>
</div>