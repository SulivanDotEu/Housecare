<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Address" table="address">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="street" type="string" column="street" length="255" nullable="false"/>
    <field name="number" type="string" column="number" length="50" nullable="false"/>
    <field name="zipcode" type="string" column="zipCode" length="50" nullable="false"/>
    <field name="country" type="string" column="country" length="255" nullable="false"/>
    <field name="comment" type="text" column="comment" nullable="false"/>
  </entity>
</doctrine-mapping>
