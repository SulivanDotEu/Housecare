<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Workers" table="workers">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstName" length="30" nullable="false"/>
    <field name="gsm" type="string" column="gsm" length="20" nullable="false"/>
    <field name="maison" type="string" column="maison" length="20" nullable="false"/>
    <field name="horaires" type="string" column="horaires" length="255" nullable="false"/>
    <field name="gsmsauv" type="string" column="gsmSAUV" length="20" nullable="false"/>
    <field name="ordre" type="integer" column="ordre" nullable="false"/>
    <field name="wnotes" type="text" column="wNotes" nullable="false"/>
    <field name="statut" type="integer" column="statut" nullable="false"/>
  </entity>
</doctrine-mapping>
