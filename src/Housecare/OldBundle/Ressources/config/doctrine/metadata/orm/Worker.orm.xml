<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Worker" table="worker">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="email" type="string" column="email" length="255" nullable="false"/>
    <field name="firstname" type="string" column="firstName" length="255" nullable="false"/>
    <field name="lastname" type="string" column="lastName" length="255" nullable="false"/>
    <field name="phone" type="string" column="phone" length="255" nullable="false"/>
    <field name="position" type="smallint" column="position" nullable="false"/>
    <field name="note" type="text" column="note" nullable="false"/>
    <field name="statut" type="smallint" column="statut" nullable="false"/>
    <field name="zipcode" type="string" column="zipCode" length="255" nullable="false"/>
    <field name="qualification" type="smallint" column="qualification" nullable="false"/>
  </entity>
</doctrine-mapping>
